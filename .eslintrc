{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "airbnb-base",
    "rules":{
      "consistent-return": 0,
      "import/no-named-default": 0,
      "import/prefer-default-export": 0,
      "no-bitwise": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0
    }
}
